import java.util.Scanner;
class Pattern10 
{
	public static void main(String[] args) 
	{
	Scanner scn=new Scanner(System.in);
	System.out.println("enter the N value");
	int n=scn.nextInt();
	System.out.println("this is 10th Pattern");
	for(int i=1;i<=n;i++)
		{
		for(int j=1;j<=n;j++)
			{
			if(i+j>5)
				{
				System.out.print("*");
				}
				else
				{
					System.out.print(" ");
				}
			}
			System.out.println();
		}
	}
}